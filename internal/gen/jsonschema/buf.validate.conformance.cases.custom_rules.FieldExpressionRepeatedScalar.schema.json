{
  "$id": "buf.validate.conformance.cases.custom_rules.FieldExpressionRepeatedScalar.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": true,
  "properties": {
    "val": {
      "items": {
        "anyOf": [
          {
            "maximum": 2147483647,
            "minimum": -2147483648,
            "type": "integer"
          },
          {
            "pattern": "^-?[0-9]+$",
            "type": "string"
          }
        ]
      },
      "type": "array"
    }
  },
  "title": "Field Expression Repeated Scalar",
  "type": "object"
}
