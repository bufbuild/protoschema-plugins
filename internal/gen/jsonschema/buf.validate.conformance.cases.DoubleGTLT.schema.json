{
  "$id": "buf.validate.conformance.cases.DoubleGTLT.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": true,
  "properties": {
    "val": {
      "anyOf": [
        {
          "exclusiveMaximum": 10,
          "exclusiveMinimum": 0,
          "type": "number"
        },
        {
          "pattern": "^-?[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?$",
          "type": "string"
        }
      ],
      "default": 0
    }
  },
  "title": "DoubleGTLT",
  "type": "object"
}
