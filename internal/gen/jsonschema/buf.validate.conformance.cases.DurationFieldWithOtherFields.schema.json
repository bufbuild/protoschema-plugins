{
  "$id": "buf.validate.conformance.cases.DurationFieldWithOtherFields.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": true,
  "description": "Regression for earlier bug where missing Duration field would short circuit\n evaluation in C++.",
  "patternProperties": {
    "^(durationVal)$": {
      "$ref": "google.protobuf.Duration.schema.json"
    },
    "^(intVal)$": {
      "anyOf": [
        {
          "exclusiveMaximum": 2147483648,
          "exclusiveMinimum": 16,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    }
  },
  "properties": {
    "duration_val": {
      "$ref": "google.protobuf.Duration.schema.json"
    },
    "int_val": {
      "anyOf": [
        {
          "exclusiveMaximum": 2147483648,
          "exclusiveMinimum": 16,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    }
  },
  "title": "Duration Field With Other Fields",
  "type": "object"
}
